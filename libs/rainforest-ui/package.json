{
  "name": "@rainforest-dev/rainforest-ui",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./tailwindcss/*": {
      "types": "./dist/tailwindcss/*.d.ts",
      "import": "./dist/tailwindcss/*.js",
      "default": "./dist/tailwindcss/*.cjs"
    },
    "./lit/*": {
      "types": "./dist/lit/*.d.ts",
      "import": "./dist/lit/*.js",
      "default": "./dist/lit/*.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "@tailwindcss/typography": "^0.5.16",
    "lit": "3.3.0",
    "tailwindcss": "4.1.10"
  },
  "nx": {
    "sourceRoot": "libs/rainforest-ui/src",
    "name": "rainforest-ui"
  },
  "devDependencies": {
    "@storybook/test": "^8.6.14",
    "@storybook/web-components": "^9.0.12",
    "@tailwindcss/vite": "4.1.10",
    "@types/node": "^24.0.3",
    "eslint-plugin-lit": "^2.1.1",
    "glob": "^11.0.3",
    "vite-bundle-analyzer": "^0.22.3"
  }
}
